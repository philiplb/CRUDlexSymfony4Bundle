{
    "name": "philiplb/crudlexsymfony4bundle",
    "type": "symfony-bundle",
    "description": "Symfony 4 integration of CRUDlex.",
    "keywords": ["symfony", "crud", "admin generator"],
    "homepage": "https://github.com/philiplb/CRUDlexSymfony4Bundle",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/philiplb/sphpdox"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ptlis/MetaYaml"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "philiplb/crudlex": "dev-master",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/extensions": "^1.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "philiplb\\": "."
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
