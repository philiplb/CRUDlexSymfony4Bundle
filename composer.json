{
    "name": "philiplb/crudlexsymfony4bundle",
    "type": "symfony-bundle",
    "description": "Symfony 4 integration of CRUDlex.",
    "keywords": ["symfony", "crud", "admin generator"],
    "homepage": "https://github.com/philiplb/CRUDlexSymfony4Bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.1.3",
        "philiplb/crudlex": "0.14.0",
        "sensio/framework-extra-bundle": "^5.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/lts": "^4@dev",
        "symfony/translation": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/yaml": "^4.1",
        "twig/extensions": "^1.5"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "philiplb\\CRUDlexSymfony4Bundle\\": "."
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
